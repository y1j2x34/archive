{
    "root": true,
    "extends": "airbnb-base",
    "rules": {
        "comma-dangle": 0,
        "indent": ["error", 4],
        "object-shorthand": 0,
        "func-names": 0,
        "max-len": [1, 120, 2],
        "no-use-before-define": ["error"],
        "no-param-reassign": 0,
        "strict": 0,
        "import/no-extraneous-dependencies": 1,
        "prefer-spread": 0,
        "prefer-rest-params": 0,
        "prefer-destructuring": 0,
        "no-console": "error",
        "arrow-parens":"off",
        "class-methods-use-this": 0
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "es6": true
    }
}
